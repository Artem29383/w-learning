{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Boilerplate for study",
  "scripts": {
    "dev": "NODE_ENV=development yarn copy:worker && cross-env webpack serve --config webpack/webpack.dev.ts --hot",
    "build": "NODE_ENV=production cross-env webpack --config webpack/webpack.prod.ts",
    "start": "cross-env webpack --config webpack/webpack.prod.ts && webpack --config webpack/webpack.server.ts --mode production && node build/server_build/main.js",
    "server": "node build/server_build/main.js",
    "build:server": "webpack --config webpack/webpack.server.ts --mode production",
    "copy:worker": "node tools/scripts/copy-worker.js",
    "copy:worker-prod": "node tools/scripts/copy-worker-prod.js",
    "transpile": "babel server/index.ts --out-dir dist-server"
  },
  "keywords": [
    "webpack",
    "react",
    "pwa",
    "service-worker",
    "typescript",
    "babel",
    "server-side-rendering",
    "ssr"
  ],
  "author": "Artem29383, LinkedIn: https://www.linkedin.com/in/artem-averyanov-4836a51a5/",
  "license": "MIT",
